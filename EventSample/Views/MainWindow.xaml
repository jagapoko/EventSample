<Window x:Class="EventSample.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="400" Width="525" >

    <StackPanel >
        <ContentControl prism:RegionManager.RegionName="CodeBehind" />
        <Separator Height="20" />
        <ContentControl prism:RegionManager.RegionName="Prism" />
        <Separator Height="20"/>
        <ContentControl prism:RegionManager.RegionName="ReactiveProperty" />
    </StackPanel>
</Window>
